
proc calculate_area(int width, int height) => {
    int area = width * height;
    out_to_console("Calculating area of rectange: ");
    result -> {area};
}

begin:
    out_to_console("Welcome to the area calculator!");
    ...

    int result = calculate_area(5, 3);

    out_to_console("The area of the rectangle is: " + result);
    ...
    out_to_console("Thank you for using the area calculator!");
end